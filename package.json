{
  "name": "zls-template",
  "version": "0.0.1",
  "description": "vite + vue3 + ts 开箱即用现代开发模板",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview --host --open",
    "typecheck": "npx vue-tsc --noEmit",
    "deps:fresh": "npx taze -I -w",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "auto:create": "plop --plopfile scripts/create.js",
    "auto:remove": "plop --plopfile scripts/remove.js",
    "lint": "eslint --ext .ts,.js,.jsx,.vue .",
    "lint:fix": "eslint --fix --ext .ts,.js,.jsx,.vue .",
    "release": "npx bumpp -r && npm -r publish --access public",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sohaha/zlog": "^0.0.3",
    "@vueuse/core": "^9.13.0",
    "@vueuse/motion": "2.0.0-beta.26",
    "ayyui": "^0.1.14",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.4",
    "push.js": "^1.0.12",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-use-api": "^0.0.8"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.2",
    "@iconify-json/bx": "^1.1.6",
    "@intlify/unplugin-vue-i18n": "^0.8.2",
    "@sergeymakinen/vite-plugin-html-minimize": "^1.0.6",
    "@types/node": "18.11.13",
    "@unocss/preset-icons": "^0.47.6",
    "@unocss/preset-wind": "^0.47.6",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "echarts": "^5.4.3",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "fast-glob": "^3.3.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "local-pkg": "^0.4.3",
    "md-editor-v3": "^2.11.3",
    "mockjs": "^1.1.0",
    "pinia-plugin-persistedstate": "^3.1.0",
    "plop": "^3.1.2",
    "postcss-preset-env": "^7.8.3",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.2",
    "terser": "^5.19.1",
    "typescript": "4.9.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.4.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-inspect": "^0.7.33",
    "vite-plugin-mock": "^2.9.8",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-removelog": "^0.2.1",
    "vite-plugin-restart": "^0.2.0",
    "vite-plugin-vue-devtools": "^0.3.2",
    "vite-plugin-vue-meta-layouts": "^0.1.2",
    "vue-echarts": "^6.6.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-i18n": "^9.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "markdown-it",
        "@types/markdown-it"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "npm run lint:fix"
  }
}
