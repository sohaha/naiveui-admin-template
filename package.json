{
  "name": "zls-template",
  "version": "0.0.1",
  "description": "vite + vue3 + ts 开箱即用现代开发模板",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview --host --open",
    "typecheck": "npx vue-tsc --noEmit",
    "deps:fresh": "npx taze -I -w",
    "deps:fresh:major": "npx taze major -I",
    "deps:fresh:minor": "npx taze minor -I",
    "deps:fresh:patch": "npx taze patch -I",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "auto:create": "plop --plopfile scripts/create.js",
    "auto:remove": "plop --plopfile scripts/remove.js",
    "lint": "eslint --ext .ts,.js,.jsx,.vue .",
    "lint:fix": "eslint --fix --ext .ts,.js,.jsx,.vue .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "ayyui": "^0.1.14",
    "hint.css": "^2.7.0",
    "naive-ui": "^2.34.2",
    "pinia": "^2.0.27",
    "vue": "3.2.40"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.4",
    "@iconify-json/bx": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@sergeymakinen/vite-plugin-html-minimize": "^1.0.4",
    "@types/node": "18.11.7",
    "@unocss/preset-icons": "^0.46.5",
    "@unocss/preset-wind": "^0.46.5",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.0",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "local-pkg": "^0.4.2",
    "md-editor-v3": "^2.5.0",
    "mockjs": "^1.1.0",
    "pinia-plugin-persistedstate": "^2.4.0",
    "plop": "^3.1.1",
    "postcss-preset-env": "^7.8.3",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.0",
    "prism-theme-vars": "^0.2.4",
    "terser": "^5.16.1",
    "typescript": "4.8.4",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-inspect": "^0.7.9",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-removelog": "^0.1.0",
    "vite-plugin-restart": "^0.2.0",
    "vite-plugin-use-modules": "^1.3.1",
    "vite-plugin-vue-meta-layouts": "^0.1.1",
    "vue-echarts": "^6.2.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-use-api": "^0.0.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "markdown-it",
        "@types/markdown-it"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "npm run lint:fix"
  }
}
